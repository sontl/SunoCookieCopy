{
    "manifest_version": 3,
    "name": "Suno Cookie Copier",
    "version": "1.0",
    "permissions": [
      "cookies",
      "storage",
      "declarativeNetRequest",
      "declarativeNetRequestFeedback",
      "tabs"
    ],
    "host_permissions": [
      "https://*.suno.com/*",
      "https://clerk.suno.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
